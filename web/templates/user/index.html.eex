<section class="page">
  <article>
    <p>Logged in as: <%= @session %></p>
    <hr />
    <h5>Users</h5>
    <table>
      <tbody>
        <%= for user <- @users do %>
          <tr>
            <td><%= user.email %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <h5>Signups</h5>
    <table>
      <tbody>
        <%= for signup <- @signups do %>
          <tr>
            <td><%= signup.email %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <a href="<%= user_path(@conn, :signups) %>" class="signups">Download CSV</a>
  </article>
  <aside>
    <a href="<%= user_path(@conn, :logout) %>" class="logout">Logout</a>
  </aside>
</section>
